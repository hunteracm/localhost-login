{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run build",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "start": "node ./build/bin/www.js",
    "transpile": "babel ./src --out-dir build",
    "clean": "rimraf build",
    "build": "npm run clean && npm run transpile",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.17.3",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.11",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
